                   android:text="@string/glob_empty_container"
                   android:textAppearance="?android:attr/textAppearanceMedium" />
           </TableRow>

           <TableRow
               android:id="@+id/tableRow5"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content" >

